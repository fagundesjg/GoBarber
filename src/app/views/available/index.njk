{% for hour in available %}
  <label for="time-{{hour.time}}" class="{% if hour.available === false %}disabled{% endif %}" >
    {% if hour.available === true %}
      <input name="date" type="radio" id="time-{{hour.time}}" value="{{hour.value}}"/>
    {% endif %}
    {{ hour.time }}
  </label>
{% endfor %}
